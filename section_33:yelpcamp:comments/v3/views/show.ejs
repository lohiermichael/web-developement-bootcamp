<%- include('partials/header') %>
    <h1><%= campground.name %></h1>

    <img src="<%= campground.image %> " alt="">

    <p>
        <%= campground.description %> 
    </p>
    <% for (comment of campground.comments){ %> 
        <p>
            <strong>
                <%= comment.author %>
            </strong> 
            - <%= comment.text %>
        </p>
    <% }; %> 
<%- include('partials/footer') %>